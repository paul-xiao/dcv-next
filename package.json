{
  "name": "dcv-next",
  "private": false,
  "version": "0.0.10",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "yarn tailwind && rimraf dist && vue-tsc && vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "prepare": "husky install",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,examples}/**/*.{vue,ts,tsx}\" --fix",
    "lint:css": "stylelint --cache --fix \"{src,examples}/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:css",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint:prettier": "prettier --write  \"{src,examples}/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prod": "npm run build && yarn version && npm publish",
    "tailwind": "tailwindcss -o src/styles/tailwind.css --minify"
  },
  "dependencies": {
    "@pxfed/dcui": "^1.0.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@algolia/client-search": ">= 4.9.1 < 6",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@element-plus/icons-vue": "^1.1.4",
    "@iconify/iconify": "^2.2.1",
    "@iconify/json": "^2.1.28",
    "@purge-icons/generated": "^0.8.1",
    "@tailwindcss/typography": "^0.5.7",
    "@types/lodash-es": "^4.17.6",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.14",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vueuse/core": "^8.3.1",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "css-doodle": "^0.30.3",
    "element-plus": "^2.2.25",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "highlight.js": "^11.6.0",
    "husky": "^7.0.0",
    "js-cookie": "^3.0.1",
    "lint-staged": "^12.3.7",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-toc-and-anchor": "^4.2.0",
    "marked": "^4.0.14",
    "mavon-editor": "3.0.0-beta",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "primeflex": "^3.1.2",
    "primeicons": "^5.0.0",
    "primevue": "^3.20.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.0",
    "sass": "^1.49.7",
    "stylelint": "^14.6.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.4.4",
    "vite": "^2.8.6",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-md": "^0.12.4",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-purge-icons": "^0.8.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitepress": "^1.0.0-alpha.29",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13",
    "vue-tsc": "^0.29.8",
    "vuex": "^4.0.2"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "element-plus": "^2.2.25"
  },
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./*": "./*"
  }
}

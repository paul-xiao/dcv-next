{
  "name": "dcv_next",
  "private": false,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vue-tsc && vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "prepare": "husky install",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,examples}/**/*.{vue,ts,tsx}\" --fix",
    "lint:css": "stylelint --cache --fix \"{src,examples}/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:css",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.4",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "primeflex": "^3.1.2",
    "primeicons": "^5.0.0",
    "primevue": "^3.11.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "stylelint": "^14.6.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.4.4",
    "vite": "^2.8.6",
    "vitepress": "^0.21.6",
    "vue": "^3.2.25",
    "vue-tsc": "^0.29.8"
  },
  "files": [
    "src",
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "module": "./dist/my-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}
